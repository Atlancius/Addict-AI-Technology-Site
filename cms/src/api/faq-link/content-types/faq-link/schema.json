{
  "kind": "collectionType",
  "collectionName": "faq_links",
  "info": {
    "singularName": "faq-link",
    "pluralName": "faq-links",
    "displayName": "FAQ Link",
    "description": "Association FAQ -> pages/services/formations"
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "target_type": {
      "type": "enumeration",
      "enum": ["page", "service", "training"],
      "required": true
    },
    "target_key": {
      "type": "string",
      "required": true
    },
    "sort_order": {
      "type": "integer",
      "default": 0,
      "required": true
    },
    "faq": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::faq.faq"
    }
  }
}
